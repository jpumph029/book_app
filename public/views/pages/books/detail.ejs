<section class="book-view container">
  <ul id="book-list">
    <% bookDetails.forEach(book => { %>
         <li class="book-item" data-id="<%= book.id %>">
           <img src =<%= book.image_url %>>
           <p><%= book.title %></p>
           <p> Author: <%= book.author %></p>
           <p> ISBN: <%= book.isbn %></p>
           <p> Description: <%= book.description %></p>
           <p> Bookshelf: <%= book.bookshelf%></p>
           <a href="/">Return</a>
         </li>
    <% }) %>
  </ul>     
</section>
