<!DOCTYPE html>
<html>
<head>
  <title>Search Results</title>
</head>
<body>
  <h1>Search Results</h1>
  <ul>
    <% books.forEach( book => { %>
      <li>
        <img src="<%= book.thumbnail %>">
        <p>Title: <%= book.title %></p>
        <p>Author: <%= book.author %></p>
        <p>Description: <%= book.description %></p>
        <a href="/book-details/ <%= book.id%>">View Details</a>
      </li>
    <% }) %>
  </ul>
</body>
</html>